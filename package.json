{
  "name": "deployable-assets",
  "version": "0.9.5-4",
  "description": "Deployable web asset management",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha",
    "test:debug": "DEBUG='*' mocha debug",
    "test:coverage": "istanbul cover _mocha && istanbul check-coverage",
    "test:gulp": "gulp --gulpfile test/fixture/gulpfile.js assets",
    "npm:pubtest": "set -uex; npm test; npm version prerelease; npm publish --tag testing",
    "npm:publish": "set -uex; rm -rf node_modules; yarn; npm run test:coverage; [ -z \"$(git status --porcelain)\" ]; npm version patch; git push; npm publish; git push --tags;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deployable/node-deployable-assets.git"
  },
  "keywords": [
    "deployable",
    "module",
    "web",
    "asset",
    "assets",
    "gulp",
    "sass",
    "babel"
  ],
  "author": "Matt Hoyle <code at deployable.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deployable/node-deployable-assets/issues"
  },
  "homepage": "https://github.com/deployable/deployable-assets#readme",
  "devDependencies": {
    "chai": "~3.5.0",
    "deployable-asset": "~0.9.1",
    "istanbul": "~0.4.5",
    "mocha": "~3.1.2",
    "sinon": "~1.17.6"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "deployable-errors": "~0.8.0",
    "gulp": "~3.9.1",
    "gulp-babel": "~6.1.2",
    "gulp-batch": "~1.0.5",
    "gulp-plumber": "~1.1.0",
    "gulp-sass": "~3.1.0",
    "gulp-sequence": "~0.4.6",
    "gulp-shell": "~0.5.2",
    "gulp-watch": "~4.3.11"
  }
}

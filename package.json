  },
  "homepage": "https://github.com/deployable/deployable-assets-gulp#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "gulp": "~3.9.1",
    "gulp-babel": "~6.1.2",
    "gulp-batch": "~1.0.5",
    "gulp-plumber": "~1.1.0",
    "gulp-sass": "~3.1.0",
    "gulp-sequence": "~0.4.6",
    "gulp-shell": "~0.5.2",
    "gulp-watch": "~4.3.11",
    "mocha": "^3.1.2",
